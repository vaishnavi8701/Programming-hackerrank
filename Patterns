n=int(input())

for i in range(0,n):
    print(" "*(((n*2)-2)-(i*2)),end="")
    for k in range(((n+1)-(n-i)),1,-1):
        print(k,end=" ")
    for j in range(1,i+2):
        print(j,end=" ")
    print(" ")
for i in range(n-1,0,-1):
    print(" "*((n-i)*2),end="")
    for k in range((n-(n-i)),1,-1):
        print(k,end=" ")
    for j in range(1,i+1):
        print(j,end=" ")
    print(" ")

