for i in range(1,6):
    print(" "*(5-i),end="")
    for j in range(i):
        print(i,end=" ")
    print(" ")
for i in range(4,0,-1):
    print(" "*(5-(i)),end="")
    for j in range(i):
        print(i,end=" ")
    print(" ")

