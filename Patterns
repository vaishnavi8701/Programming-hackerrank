for i in range(1,5):
    print(" "*((5-i)*2),end="")
    for j in range(i,0,-1):
        print(j,end=" ")
    print(" ")

for i in range(0,5):
    print(" "*(i*2),end="")
    for j in range(5-i,0,-1):
        print(j,end=" ")
    print(" ")


